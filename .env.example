# AWS S3 Configuration
AWS_S3_BUCKET=github-api0-upload
AWS_REGION=us-east-2
# Enable S3 folder partitioning: yyyy/mm/dd/ structure (true/false)
S3_USE_DATE_PARTITIONING=true

# GitHub API Configuration (optional - leave empty for unauthenticated)
GITHUB_TOKEN=

# Extraction Settings
PAGES_TO_EXTRACT=2
REPOS_PER_PAGE=50

# Cache Settings
# Cache enables incremental page loading - already cached pages are skipped
USE_CACHE=true
CACHE_DIR=cache

# Logging
LOG_LEVEL=INFO
LOG_DIR=logs
